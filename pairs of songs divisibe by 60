#include <vector>
#include <numeric> 
class Solution {
public:
    int numPairsDivisibleBy60(std::vector<int>& time) {
        int count[60] = {0};
        
        
        for (int t : time) {
            count[t % 60]++;
        }
        
        int ans = 0;
        
       
        ans += (long long)count[0] * (count[0] - 1) / 2;
        
      
        ans += (long long)count[30] * (count[30] - 1) / 2;
        
       
        for (int x = 1; x < 30; ++x) {
            ans += (long long)count[x] * count[60 - x];
        }
        
        return ans;
    }
};
