  int bus[1001] = {0};
    for(int i = 0; i < tripsSize; i++) {
        int Passenger = trips[i][0];
        int from = trips[i][1];
        int to = trips[i][2];
        bus[from] += Passenger;
        bus[to] -= Passenger;
    }
    for(int i = 1; i < 1001; i++) {  
        bus[i] += bus[i - 1];
    }
    for(int i = 0; i < 1001; i++) {
        if(bus[i] > capacity) {
            return false;
        }
    }
    return true;
}
